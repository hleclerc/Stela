class Range[ TB, TE, TI ]
    beg ~= TB
    end ~= TE
    inc ~= TI
    def init( b, e, i ) starts_with beg( b ), end( e ), inc( i )
        #
    def __for__ block
        cur := beg
        while cur < end
            ___disp beg
            ___disp end
            ___disp inc
            block cur
            cur += inc
    
